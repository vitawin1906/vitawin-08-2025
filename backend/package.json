{
  "name": "vitawin-backend",
  "version": "1.0.0",
  "description": "VitaWin Backend (Node.js, Express, Drizzle ORM, PostgreSQL)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Add your linter script here\"",
    "db:init": "psql \"$DATABASE_URL\" -f ./database/01-init.sql",
    "db:insert": "psql \"$DATABASE_URL\" -f ./database/vitawin_insert.sql",
    "db:recalc-mlm": "psql \"$DATABASE_URL\" -f ./database/cron_mlm_monthly_recalculate.sql",
    "db:process-order": "psql \"$DATABASE_URL\" -f ./database/process-order-mlm.sql"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "node",
    "express",
    "drizzle",
    "postgresql",
    "vitawin",
    "backend"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
